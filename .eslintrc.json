{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "import/no-dynamic-require": 0,
        "consistent-return": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "prefer-arrow-callback":  0
    }
}